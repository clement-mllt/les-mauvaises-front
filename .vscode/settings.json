{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#9a81bc",
    "activityBar.background": "#9a81bc",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#decdc1",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#9a81bc",
    "statusBar.background": "#7f60aa",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#9a81bc",
    "statusBarItem.remoteBackground": "#7f60aa",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#7f60aa",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#7f60aa99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#7f60aa",
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "ngClass",
    ".*Classes.*",
    ".*ClassName.*",
    "tw",
    "cx",
    "cn"
  ],
  "tailwindCSS.experimental.classRegex": [
    ["cn\\(([^)]*)\\)", "'([^']*)'"], // Capture les classes dans 'cn()' avec des simples quotes
    ["cn\\(([^)]*)\\)", "\"([^\"]*)\""], // Capture les classes dans "cn()" avec des doubles quotes
    ["cn\\(([^)]*)\\)", "`([^`]*)`"] // Capture les classes dans `cn()` avec des backticks
  ]
}
